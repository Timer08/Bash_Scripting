#wget www.megacorpone.com
#cat index.html  | grep -i ".com" | cut -d "/" -f3 | grep -i ".com$" | sort | uniq > /home/kali/bash-scripts/sub-domains


#!/bin/bash

echo "Enter the filename"
read file 

if [ -n "$file" ]
then
        if [ -f $file ]
        then
                for ip in $(cat $file)
                do
                        host $ip | cut -d " " -f1,4 | uniq
                done
        else
                echo "File do not exist"
        fi
else
        echo "File field cannot be empty"
fi
